<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="android_upgrading_title">Mise à jour de Dirty Unicorns\u2026</string>
    <string name="android_start_title">Démarrage de Dirty Unicorns en cours\u2026</string>
    <!-- [CHAR LIMIT=NONE] Message shown in upgrading dialog for each .apk that is optimized. -->
    <string name="android_upgrading_apk">Dirtyfication de l\'application
        <xliff:g id="number" example="123">%1$d</xliff:g> sur
        <xliff:g id="number" example="123">%2$d</xliff:g>.</string>

    <!-- Button to reboot the phone, within the Phone Options dialog -->
    <string name="reboot_system" product="tablet">Redémarrer la tablette</string>
    <string name="reboot_system" product="default">Redémarrer le téléphone</string>

    <!-- Button to reboot the phone, within the Reboot Options dialog -->
    <string name="reboot_reboot">Redémarrer</string>
    <!-- Button to reboot the phone into recovery, within the Reboot Options dialog -->
    <string name="reboot_recovery">Recovery</string>
    <!-- Button to reboot the phone into bootloader, within the Reboot Options dialog -->
    <string name="reboot_bootloader">Bootloader</string>
    <!-- Button to hot reboot the device, within the Reboot Options dialog -->
    <string name="reboot_hot">Hot reboot</string>
    <!-- Button to reboot the phone into bootmenu, within the Reboot Options dialog -->
    <string name="reboot_bootmenu">Bootmenu</string>
    <!-- Button to reboot the phone into fastboot, within the Reboot Options dialog -->
    <string name="reboot_fastboot">Fastboot</string>
    <!-- Button to reboot the phone into download, within the Reboot Options dialog -->
    <string name="reboot_download">Download</string>

    <!-- Reboot Progress Dialog. This is shown if the user chooses to reboot the phone. -->
    <string name="reboot_progress">Redémarrage\u2026</string>
    <!-- Reboot Confirmation Dialog.  When the user chooses to reboot the phone, there will be a confirmation dialog.  This is the message. -->
    <string name="reboot_confirm">Votre téléphone va redémarrer</string>

    <!-- label for item that reboots the phone in phone options dialog -->
    <string name="global_action_reboot">Redémarrer</string>

    <!-- screenshot in the power menu -->
    <string name="powermenu_screenshot">Capture d\'écran</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_accessThemeService">accéder au gestionnaire de thème</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessThemeService">Permet d\'accéder au gestionnaire de thème. Ne doit pas être utilisé par des applications normales.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_readThemes">lire les informations du thème</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_readThemesDesc">Permet à une application de lister vos thèmes et de déterminer celui que vous avez appliqué.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_writeThemes">modifier vos thèmes</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_writeThemesDesc">Permet à une application d\'installer de nouveaux thèmes et modifier le thème que vous avez appliqué.</string>

    <!-- Theme installation error notification -->
    <string name="theme_install_error_title">Échec d\'installation du thème</string>
    <string name="theme_install_error_message">Impossible d\'installer %s</string>
    <string name="theme_reset_notification_title">Réinitialisation du thème</string>
    <string name="theme_reset_notification_body">Thème système restauré en raison de multiples plantages d\'application.</string>
</resources>
